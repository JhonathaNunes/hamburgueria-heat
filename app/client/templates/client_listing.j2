{% extends "listing_base.j2" %}
{% block table_body %}
{% for row in data %}
    <tr>
        <td>
            {{row.full_name}}
        </td>
        <td>
            {{row.email}}
        </td>
        <td>
            {{row.phone}}
        </td>
        <td>
            {{row.cpf}}
        </td>
        <td>
            <a href="/admin/client/{{row.id}}">
                <img src="/static/images/eye.svg" alt="Visualizar"/>
            </a>
        </td>
    </tr>
{% endfor %}
{% endblock  %}
